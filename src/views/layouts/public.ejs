    <style>
        .axil-product>.thumbnail>a img {
            height: 290px;
            object-fit: scale-down;
            background-color: #fff;
        }
        .axil-product>.thumbnail>a {
            background-color: #fff;
        }
        .single-product-thumbnail .thumbnail img {
            max-height: 600px;
            object-fit: scale-down;
        }
